<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="600" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codetreatise.controller.DashboardController">

   <top>
      <MenuBar BorderPane.alignment="TOP_LEFT">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#enrollApplication" text="Enroll Application" />
               </items>
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#showApplication" text="Show Application" />
               </items>
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#uploadApplication" text="Upload Application" />
               </items>

               <items>
                  <MenuItem mnemonicParsing="false" onAction="#dashboard" text="Dashboard" />
               </items>

<!--               <items>-->
<!--                  <MenuItem mnemonicParsing="false" onAction="#createUser" text="Create User" />-->
<!--               </items>-->

               <items>
                  <MenuItem mnemonicParsing="false" onAction="#logout" text="Logout" />
               </items>

               <items>
                  <MenuItem mnemonicParsing="false" onAction="#exit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>

   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="btnOne" fitHeight="138.0" fitWidth="138.0" layoutX="100.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/1.jpeg" />
               </image>
            </ImageView>
            <ImageView fx:id="btnTwo" fitHeight="138.0" fitWidth="138.0" layoutX="388.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/3.jpg" />
               </image>
            </ImageView>
            <ImageView fx:id="btnThree" fitHeight="138.0" fitWidth="138.0" layoutX="100.0" layoutY="368.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/2.jpg" />
               </image>
            </ImageView>
            <ImageView fx:id="btnFour" fitHeight="138.0" fitWidth="138.0" layoutX="392.0" layoutY="368.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/4.jpg" />
               </image>
            </ImageView>
            <ImageView fx:id="btnSettings" fitHeight="46.0" fitWidth="66.0" layoutX="720.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
